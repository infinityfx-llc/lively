		"@types/react-dom": "npm:types-react-dom@^19.0.0-rc.0",
		"react": "^19.0.0-rc-6f23540c7d-20240528",
		"react-dom": "^19.0.0-rc-6f23540c7d-20240528",
		"rollup": "^4.17.2",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-preserve-directives": "^0.4.0",
		"typescript": "^5.4.5"
