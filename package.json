{
	"name": "@infinityfx/lively",
	"version": "4.0.0-beta.1",
	"type": "module",
	"description": "Feature complete, lightweight react animation library.",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./animations": "./dist/animations.js",
		"./hooks": "./dist/hooks.js",
		"./layout": "./dist/layout.js"
	},
	"typesVersions": {
		"*": {
			"animations": [
				"./dist/types/animations.d.ts"
			],
			"hooks": [
				"./dist/types/hooks.d.ts"
			],
			"layout": [
				"./dist/types/layout.d.ts"
			]
		}
	},
	"repository": "infinityfx-llc/lively",
	"author": "InfinityFX <contact@infinity-fx.com> (https://infinityfx.dev)",
	"license": "GPL-3.0",
	"bugs": "https://github.com/infinityfx-llc/lively/issues",
	"homepage": "https://github.com/infinityfx-llc/lively#readme",
	"funding": "https://ko-fi.com/infinityfx",
	"keywords": [
		"animation",
		"keyframe",
		"react",
		"transition",
		"morph",
		"animate",
		"reactive",
		"lightweight",
		"performant"
