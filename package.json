{
	"name": "@infinityfx/lively",
	"version": "3.0.0",
	"type": "module",
	"description": "Feature complete, lightweight react animation library.",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./animations": {
			"import": "./dist/esm/animations.js",
			"require": "./dist/cjs/animations.js"
		},
		"./hooks": {
			"import": "./dist/esm/hooks.js",
			"require": "./dist/cjs/hooks.js"
		}
	},
	"typesVersions": {
		"*": {
			"animations": [
				"./dist/types/animations.d.ts"
			],
			"hooks": [
				"./dist/types/hooks.d.ts"
			]
		}
	},
	"repository": "infinityfx-llc/lively",
	"author": "InfinityFX <contact@infinity-fx.com> (https://infinity-fx.com)",
	"license": "GPL-3.0",
	"bugs": "https://github.com/infinityfx-llc/lively/issues",
	"homepage": "https://github.com/infinityfx-llc/lively#readme",
	"funding": "https://ko-fi.com/infinityfx",
	"keywords": [
		"animation",
		"keyframe",
		"react",
		"transition",
		"morph",
		"animate",
		"reactive",
		"lightweight",
		"performant"
	],
	"scripts": {
		"build": "rollup -c --environment NODE_ENV:production",
		"watch": "rollup -c -w"
	},
	"peerDependencies": {
		"@babel/runtime": "^7.0.0",
		"react": ">=16.0.0",
		"react-dom": ">=16.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-typescript": "^11.1.0",
		"@types/react": "^18.0.33",
		"react": "^18.2.0",
		"rollup": "^3.20.2",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4"
	},
	"sideEffects": false
}
